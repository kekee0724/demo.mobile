{
    "extends": ["react-app", "react-app/jest", "alloy", "alloy/react", "alloy/typescript"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint/eslint-plugin"],
    "settings": { "react": { "version": "detect" } },
    "rules": {
        "@typescript-eslint/consistent-type-assertions": "off",
        "@typescript-eslint/consistent-type-definitions": "off",
        "@typescript-eslint/explicit-member-accessibility": "off",
        "@typescript-eslint/member-ordering": "off",
        "@typescript-eslint/method-signature-style": "off",
        "@typescript-eslint/no-duplicate-imports": "off",
        "@typescript-eslint/no-empty-interface": "off",
        "@typescript-eslint/no-inferrable-types": "off",
        "@typescript-eslint/no-invalid-this": "off",
        "@typescript-eslint/no-invalid-void-type": "off",
        "@typescript-eslint/no-namespace": "off",
        "@typescript-eslint/no-parameter-properties": "off",
        "@typescript-eslint/no-this-alias": "off",
        "@typescript-eslint/no-unused-expressions": "off",
        "@typescript-eslint/no-useless-constructor": "off",
        "@typescript-eslint/prefer-for-of": "off",
        "@typescript-eslint/prefer-function-type": "off",
        "@typescript-eslint/prefer-optional-chain": "off",
        "@typescript-eslint/typedef": "off",
        "accessor-pairs": "off",
        "array-callback-return": "off",
        "complexity": "off",
        "consistent-type-assertions": "off",
        "consistent-type-definitions": "off",
        "default-case-last": "off",
        "eqeqeq": "off",
        "explicit-member-accessibility": "off",
        "func-name-matching": "off",
        "import/first": "off",
        "jsx-a11y/alt-text": "off",
        "jsx-a11y/anchor-is-valid": "off",
        "jsx-a11y/iframe-has-title": "off",
        "jsx-curly-brace-presence": "off",
        "jsx-no-useless-fragment": "off",
        "max-params": "off",
        "member-ordering": "off",
        "method-signature-style": "off",
        "new-cap": "off",
        "no-duplicate-imports": "off",
        "no-empty-interface": "off",
        "no-empty-pattern": "off",
        "no-eq-null": "off",
        "no-extend-native": "off",
        "no-find-dom-node": "off",
        "no-implicit-coercion": "off",
        "no-inferrable-types": "off",
        "no-invalid-this": "off",
        "no-invalid-void-type": "off",
        "no-namespace": "off",
        "no-new": "off",
        "no-param-reassign": "off",
        "no-parameter-properties": "off",
        "no-promise-executor-return": "off",
        "no-proto": "off",
        "no-return-assign": "off",
        "no-sequences": "off",
        "no-string-refs": "off",
        "no-template-curly-in-string": "off",
        "no-this-alias": "off",
        "no-undef": "off",
        "no-unreachable": "off",
        "no-unused-expressions": "off",
        "no-useless-constructor": "off",
        "no-void": "off",
        "one-var": "off",
        "prefer-for-of": "off",
        "prefer-function-type": "off",
        "prefer-object-spread": "off",
        "prefer-optional-chain": "off",
        "prefer-promise-reject-errors": "off",
        "radix": "off",
        "react/jsx-curly-brace-presence": "off",
        "react/jsx-no-useless-fragment": "off",
        "react/no-find-dom-node": "off",
        "react/no-string-refs": "off",
        "react/self-closing-comp": "off",
        "require-yield": "off",
        "self-closing-comp": "off",
        "spaced-comment": "off",
        "typedef": "off",
        "yoda": "off",
        "no-var": "off",
        "prefer-regex-literals": "off",
        "prefer-arrow-callback": "off"
    }
}
