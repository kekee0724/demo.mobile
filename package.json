{
    "name": "my-app",
    "version": "0.1.0",
    "private": true,
    "homepage": "./",
    "scripts": {
        "clean": "npx rimraf dist-pkg dist",
        "clean:all": "npx rimraf node_modules dist",
        "admin": "node ./bin/devkit-admin",
        "start": "react-scripts start --port 3000",
        "build": "react-scripts build",
        "build:pkg": "npm run admin -- build",
        "publish": "npm run admin -- publish",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "@reco-m/ide": "~10.0.10-rc.1",
        "@reco-m/shared-ide": "~10.1.0-rc.7",
        "antd-mobile": "^5.0.0-rc.0",
        "antd-mobile-v2": "^2.3.4"
    },
    "resolutions": {
        "@types/react": "^16.9.53",
        "@types/eslint": "7.28.2"
    },
    "publishConfig": {
        "registry": "http://src.devops.bitech.cn:86/repository/RECO.NPM.PKG/"
    },
    "repository": {
        "type": "git",
        "url": "http://src.devops.bitech.cn/framework/RECO10.Park.Mobile.git"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "copy": [
        {
            "from": "src/assets",
            "to": "assets"
        }
    ],
    "prettier": {
        "printWidth": 180,
        "tabWidth": 4
    }
}
